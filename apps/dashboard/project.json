{
  "projectType": "application",
  "root": "apps/dashboard",
  "sourceRoot": "apps/dashboard/src",
  "prefix": "dashboards-app",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "customWebpackConfig": {
          "path": "webpack.browser.js"
        },
        "outputPath": "server/wwwroot/apps/dashboard",
        "index": "apps/dashboard/src/index.html",
        "main": "apps/dashboard/src/main.ts",
        "polyfills": "apps/dashboard/src/polyfills.ts",
        "tsConfig": "apps/dashboard/tsconfig.app.json",
        "assets": [
          "apps/dashboard/src/favicon.ico",
          "apps/dashboard/src/assets"
        ],
        "styles": [
          "server/wwwroot/libs/shared/dashboard-styles/dashboards.css",
          "apps/dashboard/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/dashboard/src/environments/environment.ts",
              "with": "apps/dashboard/src/environments/environment.prod.ts"
            }
          ],
          "vendorChunk": true,
          "outputHashing": "none"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "dashboard:build:production"
        },
        "development": {
          "browserTarget": "dashboard:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "dashboard:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/dashboard/src/**/*.ts",
          "apps/dashboard/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@angular-builders/custom-webpack:karma",
      "outputs": ["coverage/apps/dashboard"],
      "options": {
        "customWebpackConfig": {
          "path": "webpack.karma.js"
        },
        "main": "apps/dashboard/src/test.ts",
        "tsConfig": "apps/dashboard/tsconfig.spec.json",
        "karmaConfig": "apps/dashboard/karma.conf.js",
        "polyfills": "apps/dashboard/src/polyfills.ts",
        "styles": [
          "server/wwwroot/libs/shared/dashboard-styles/dashboards.css",
          "apps/dashboard/src/styles.scss"
        ],
        "scripts": [],
        "assets": []
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["dashboard-styles"]
}
